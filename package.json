    "jest": "^29.4.3",
    "jest-html-reporters": "^3.0.10",
    "json-server": "^0.17.3",
    "lint-staged": "^13.0.3",
    "loki": "^0.31.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "2.7.1",
    "react-refresh": "^0.14.0",
    "reg-cli": "^0.17.6",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.59.2",
    "sass-loader": "^13.2.0",
    "storybook-addon-mock": "^2.4.1",
    "storybook-addon-themes": "^6.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-loader": "^9.4.2",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^3.1.0",
    "vite-plugin-svgr": "^2.2.1",
    "webpack": "^5.76.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@react-spring/web": "^9.5.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@use-gesture/react": "^10.2.19",
    "axios": "^1.3.5",
    "concurrently": "^8.0.1",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.3",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.10.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "resolutions": {
    "@types/react": "^18.2.0"
  },
  "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-lenk/production.git"
  },
  "bugs": {
    "url": "https://github.com/alex-lenk/production/issues"
  },
  "homepage": "https://github.com/alex-lenk/production/blob/main/README.md"
}
